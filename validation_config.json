{
  "services": {
    "gateway_service": {
      "port": 8080,
      "endpoints": [
        {"path": "/health", "method": "GET", "expected_status": 200},
        {"path": "/metrics", "method": "GET", "expected_status": 200},
        {"path": "/api/v1/health", "method": "GET", "expected_status": 200}
      ]
    },
    "auth_service": {
      "port": 8001,
      "endpoints": [
        {"path": "/health", "method": "GET", "expected_status": 200},
        {"path": "/metrics", "method": "GET", "expected_status": 200},
